{
  "name": "irccloud-cli",
  "version": "0.3.1",
  "description": "irccloud commandline interface",
  "keywords": [
    "command",
    "irccloud",
    "websocket"
  ],
  "bin": {
    "ic": "./bin/ic"
  },
  "main": "./lib/index.js",
  "scripts": {
    "build:lib": "babel ./src/lib.js -o ./lib/index.js",
    "build": "npm run build:lib && babel ./src/bin.js -o ./bin/ic && chmod 755 ./bin/ic",
    "prepublish": "npm run build",
    "test": "npm run build && tape -r babel-register test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yhsiang/irccloud-cli.git"
  },
  "author": "Yuan Hsiang Cheng <lyforever62@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "cologger": "~1.1.2",
    "commander": "^2.6.0",
    "request-promise": "^2.0.0",
    "ws": "^0.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "nock": "^7.0.2"
  },
  "engines" : {
    "node" : ">=4.1.0"
  }
}
